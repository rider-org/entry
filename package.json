{
  "name": "rider",
  "version": "1.0.0",
  "description": "Rider - Entrypoint",
  "scripts": {
    "repo:clone": "tsx cloneRepos.ts",
    "git:status": "./git-status.sh",
    "install:all": "pnpm i -r",
    "update:all": "pnpm up -r \"@rider/*\"",
    "nuke:all": "find . -name \"node_modules\" -type d -prune -exec rm -rf '{}' +",
    "db:commit": "cd models && pnpm run db:commit",
    "db:migrate": "cd models && pnpm run db:migrate",
    "db:generate": "cd models && pnpm run db:generate"
  },
  "keywords": [],
  "dependencies": {
    "@types/node": "^22.13.4",
    "concurrently": "^9.1.2",
    "tsx": "^4.19.3"
  },
  "private": true,
  "packageManager": "pnpm@10.4.1+sha512.c753b6c3ad7afa13af388fa6d808035a008e30ea9993f58c6663e2bc5ff21679aa834db094987129aa4d488b86df57f7b634981b2f827cdcacc698cc0cfb88af"
}
